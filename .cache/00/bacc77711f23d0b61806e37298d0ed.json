rmat(\"woff2\"),\r\n    url(\"../font/dreamscape-webfont.woff\") format(\"woff\");\r\n  font-weight: normal;\r\n  font-style: normal;\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nhtml {\r\n  font-size: 16px;\r\n  overflow: hidden;\r\n}\r\n\r\nbody {\r\n  font-family: sans-serif;\r\n  background-color: $color-bg;\r\n}\r\n\r\na {\r\n  color: inherit;\r\n  text-decoration: none;\r\n}","src\\assets\\css\\_animations.scss":"$fade-animations: fadeIn, fadeInDown, fadeInUp, fadeInLeft, fadeInRight;\r\n\r\n@keyframes fadeIn {\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n@keyframes fadeInDown {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateY(-10px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateY(0px);\r\n  }\r\n}\r\n@keyframes fadeInUp {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateY(10px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateY(0px);\r\n  }\r\n}\r\n@keyframes fadeInLeft {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateX(10px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateX(0px);\r\n  }\r\n}\r\n@keyframes fadeInRight {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateX(-10px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateX(0px);\r\n  }\r\n}\r\n\r\n@keyframes heroTitle {\r\n  0% {\r\n    opacity: 0;\r\n    text-shadow:  2px 0px 20px rgb(36, 255, 193), \r\n                  0px 2px 20px rgb(36, 255, 193),\r\n                  -2px 0px 20px rgb(36, 255, 193), \r\n                  0px -2px 20px rgb(36, 255, 193);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    text-shadow:  2px 0px 4px rgb(36, 255, 193), \r\n                  0px 2px 4px rgb(36, 255, 193),\r\n                  -2px 0px 4px rgb(36, 255, 193),\r\n                  0px -2px 4px rgb(36, 255, 193);\r\n  }\r\n}\r\n\r\n@each $animation in $fade-animations {\r\n  .#{$animation} {\r\n    opacity: 0;\r\n    animation: #{$animation} 1s;\r\n    animation-timing-function: cubic-bezier(0.645, 0.045, 0.355, 1);\r\n    animation-fill-mode: forwards;\r\n  }\r\n}\r\n","src\\assets\\css\\_layout.scss":"canvas {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  opacity: 0;\r\n  animation: fadeInDown 1s 0s linear forwards;\r\n}\r\n\r\nheader {\r\n  position: absolute;\r\n  z-index: 2;\r\n  top: 0;\r\n  margin: 0 auto;\r\n  color: $color-text;\r\n  font-family: \"Roboto Mono\", monospace;\r\n  padding: $padding;\r\n}\r\n\r\n.left {\r\n  &__title {\r\n    font-size: 1rem;\r\n    font-weight: 400;\r\n  }\r\n  &__link {\r\n    font-weight: bold;\r\n    &:hover {\r\n      color: #c4c4c4;\r\n    }\r\n  }\r\n}\r\n\r\nnav {\r\n  display: grid;\r\n  grid-template-columns: 5fr 2fr;\r\n}\r\n\r\n.hero__title {\r\n  font-family: \"dreamscaperegular\";\r\n  font-size: 10rem;\r\n  color: rgb(77, 255, 204);\r\n  opacity: 0.6;\r\n  text-shadow: 2px 0px 20px rgb(36, 255, 193), 0px 2px 20px rgb(36, 255, 193),\r\n    -2px 0px 20px rgb(36, 255, 193), 0px -2px 20px rgb(36, 255, 193);\r\n  position: absolute;\r\n  z-index: 1;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  letter-spacing: 15px;\r\n  animation: heroTitle 1s cubic-bezier(0.68, -0.55, 0.265, 1.55) forwards;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"b04378243ea90a52ae89b6ee507ecd2f","cacheData":{"env":{}}}